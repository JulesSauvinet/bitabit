                <question ask="Quelle est la plus grande valeur qu'on puisse écrire avec 8 bits ?">
                    <answer>8</answer>
                    <answer>16</answer>
                    <answer check="true">255</answer>
                    <answer>256</answer>
                </question>

                <question ask="Que vaut 2 exposant 10 ?">
                    <answer check="true">1024</answer>
                    <answer>256</answer>
                    <answer>1000</answer>
                    <answer>65536</answer>
                </question>
